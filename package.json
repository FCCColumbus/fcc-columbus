{
  "name": "FCCC",
  "version": "0.0.1",
  "private": true,
  "description": "FreeCodeCamp Columbus website",
  "main": "index.js",
  "license": "MIT",
  "repository": "https://github.com/FCCColumbus/F3C_Website",
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon --watch webpack.config.babel.js --exec \"webpack-dev-server --hot --inline --progress\"",
    "prod:build": "rimraf lib dist && babel app -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:serve": "serve ./dist -s -o",
    "lint": "eslint app webpack.config.babel.js --ext .js,.jsx && flow",
    "fix": "eslint app webpack.config.babel.js --ext .js,.jsx --fix",
    "test": "yarn lint && flow && jest --coverage",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "engines": {
    "yarn": ">= 1.0.2"
  },
  "browserslist": [
    "> 1%"
  ],
  "keywords": [],
  "author": "freecodecamp columbus contributors",
  "dependencies": {
    "axios": "^0.16.2",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "react": "16",
    "react-dom": "16",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "compression-webpack-plugin": "^1.0.1",
    "connected-react-router": "^4.2.3",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.5.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.55.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "^3.4.2",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "jest": "^21.1.0",
    "node-sass": "^4.5.3",

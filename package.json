    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "redux-devtools": "^3.4.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "serve": "^6.3.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-chunk-hash": "^0.5.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "modulePaths": [
      "node_modules",
      "app"
    ]
  }
}
